<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<groups>
		<run>
			<!-- if use the include that named test cases are executed-->
			<include name="smoke"></include>
			<include name="sanity"></include>
			
			<!-- if use the exclude that named test cases are not executed-->
			<exclude name="reg"></exclude>
		</run>
	</groups>
  <test thread-count="5" name="Test">
    <classes>
      <class name="TestNG.LearnAttribute6"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
